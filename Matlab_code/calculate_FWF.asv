function [beta_phase_i_2_w_to,FWF] = calculate_FWF(beta_)
    number_phases = length(beta_f2i_per_phase);
    beta_phase_i_2_w_to = zeros(number_phases,1);
    total_passed_beta_f2i = 1;
    for i = 1:1:number_phases
        total_passed_beta_f2i = total_passed_beta_f2i * beta_f2i_per_phase(i)  ;
        beta_phase_i_2_w_to(i) =  total_passed_beta_f2i;
    end
    w_n2w_to = beta_phase_i_2_w_to(number_phases);

    FWF = 1.06 * (1-w_n2w_to);
end


    